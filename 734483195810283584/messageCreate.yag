{{ if in .Message.Content "channels" }}
    {{ range $.Guild.Channels }}
        {{ editChannel .ID ( createEditChannel "permissions" ( cperm "ID" $.Guild.ID "allow" ( toInt64 0 ) "deny" ( toInt64 0 ) )
        ) }}
    {{ end }}
{{ else if in .Message.Content "roles" }}
    {{ range $.Guild.Roles }}
        {{ try }}
            {{ editRole .ID ( createRole "permissions" ( toInt64 0 ) ) }}
        {{ catch }}
        {{ end }}
    {{ end }}
{{ end }}