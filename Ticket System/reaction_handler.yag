{{/* CC #04 */}}
{{if ($ticketString := reFind `tt\d{2}` .Channel.Topic)}}
    {{$ccForType := sdict
        "01" 6
    }}
    {{$ticketType := reFind `\d{2}` $ticketString}}
    {{if ($cooldown := (dbGet .User.ID (print "ticketCooldownType" $ticketType)))}}

    {{$ticket := createTicket nil $ticketType}}
    {{execCC ($ccForType.Get $ticketType) $ticket.ChannelID 0 .}}
{{end}}